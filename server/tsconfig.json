{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ES2020",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "outDir": "./dist",
      "strict": true
    },
    "include": ["server/**/*.ts"],
    "exclude": [
    "node_modules",
    "client/dist"
  ]
}
  